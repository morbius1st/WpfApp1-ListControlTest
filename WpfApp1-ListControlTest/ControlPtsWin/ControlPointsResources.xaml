		xmlns:validation="clr-namespace:WpfApp1_ListControlTest.ControlPtsWin"
		>

	<validation:ControlPointXYZConverter x:Key="Converter.Xyz" />
	<validation:ControlPointSlopeConverter x:Key="Converter.Slope" />
	<validation:PointTemplateSelector x:Key="Point.TemplateSelector" />

	<!-- <parent:ControlPoints x:Key="cps"/> -->

	<!--  Values  -->

	<!--#region > Lengths-->

	<sys:Int32 x:Key="pointRow">
		0
	</sys:Int32>
	<sys:Int32 x:Key="Row.General.Value">
		2
	</sys:Int32>

	<sys:Int32 x:Key="Field.Pt.Value.Row">
		0
	</sys:Int32>
	<sys:Int32 x:Key="Field.Pt.Data.Row">
		1
	</sys:Int32>
	<sys:Int32 x:Key="Field.Ed.Row">
