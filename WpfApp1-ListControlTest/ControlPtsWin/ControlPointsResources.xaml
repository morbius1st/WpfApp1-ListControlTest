 						<ColumnDefinition Width="*" />
 						<ColumnDefinition Width="1" />
 					</Grid.ColumnDefinitions>
 					<Grid.RowDefinitions>
 						<RowDefinition Height="0" />
 						<RowDefinition Height="*" />
 						<RowDefinition Height="0" />
 					</Grid.RowDefinitions>

 					~1~  left  @1@
 					<Border Grid.Row="0"
 							Grid.RowSpan="3"
 							Grid.Column="0"
 							BorderThickness="1,0,0,0"
 							BorderBrush="{StaticResource controlBkg}"
 							/>
 					~1~  top  @1@
 					<Border Grid.Row="0"
 							Grid.Column="0"
 							Grid.ColumnSpan="3"
 							BorderThickness="0,1,0,0"
 							BorderBrush="{StaticResource controlBkg}"
 							/>
 					~1~  right  @1@
 					<Border Grid.Row="0"
 							Grid.RowSpan="3"
 							Grid.Column="2"
 							BorderThickness="0,0,1,0"
 							BorderBrush="{StaticResource stdBorderBrush}"
 							/>
 					~1~  bottom  @1@
 					<Border Grid.Row="2"
 							Grid.Column="0"
 							Grid.ColumnSpan="3"
 							BorderThickness="0,0,0,0"
 							BorderBrush="{StaticResource controlBkg}"
 							/>

 					<ContentPresenter Grid.Row="1"
 							Grid.Column="1"
 							/>
 				</Grid>
 			</ControlTemplate>
 		</Setter.Value>
 	</Setter>
 </Style>

