	</GridLength>

	<GridLength x:Key="Col.00.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.01.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.02.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.03.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.04.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.05.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.06.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.07.Width">
		1*
	</GridLength>
	<GridLength x:Key="Col.08.Width">
		17
	</GridLength>
	<!--#endregion-->

	<!--#region > Color & Brushes-->

	<SolidColorBrush x:Key="Header.Background.C"
			Color="{StaticResource gray.250}"
			/>
	<SolidColorBrush x:Key="Header.Filler.Background.C"
			Color="{StaticResource gray.200}"
			/>
	<SolidColorBrush x:Key="Header.EndPoint.Filler.Background.C"
			Color="{StaticResource gray.400}"
			/>

	<SolidColorBrush x:Key="Point.Filler.Background.C"
			Color="{StaticResource gray.400}"
			/>

	<SolidColorBrush x:Key="Row.Edit.Background.C"
			Color="{StaticResource gray.850}"
			/>



	<SolidColorBrush x:Key="ListBox.Background.C"
			Color="{StaticResource red.050}"
			/>

	<SolidColorBrush x:Key="Brush.Border.Std.C"
			Color="{StaticResource gray.350}"
			/>
	<SolidColorBrush x:Key="Brush.Border.Light.C"
			Color="{StaticResource gray.450}"
			/>

	<SolidColorBrush x:Key="Color.Gray.Light.Very.C"
			Color="{StaticResource gray.970}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Light.Xtra.C"
			Color="{StaticResource gray.900}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Med.Light.Xtra.C"
			Color="{StaticResource gray.730}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Med.C"
			Color="{StaticResource gray.700}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Xtra.C"
			Color="{StaticResource gray.300}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Very.C"
			Color="{StaticResource gray.220}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Xtra.Very.C"
			Color="{StaticResource gray.150}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Very.Very.C"
			Color="{StaticResource gray.050}"
			/>

	<SolidColorBrush x:Key="Color.Blue.Light.Xtra.C"
			Color="{StaticResource blue.900}"
			/>
	<SolidColorBrush x:Key="Color.Blue.Dark.Xtra.C"
			Color="{StaticResource blue.gray.010}"
			/>




	<!--  general  -->

	<SolidColorBrush x:Key="Brush.Border.Std"
			Color="{Binding Color, Source={StaticResource Brush.Border.Std.C}}"
			/>

	<SolidColorBrush x:Key="Brush.Background.Std"
			Color="{Binding Color, Source={StaticResource Normal.Background.C}}"
			/>
	<SolidColorBrush x:Key="Brush.DockPanel.Background"
			Color="{Binding Color, Source={StaticResource Color.Blue.Dark.Xtra.C}}"
			/>
	<!--  <SolidColorBrush x:Key="Brush.Heading.Background"  -->
	<!--  Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Very.C}}"  -->
	<!--  />  -->
	<!--  <SolidColorBrush x:Key="Brush.Item.Disabled.Background"  -->
	<!--  Color="{Binding Color, Source={StaticResource Color.Gray.Light.Xtra.C}}"  -->
	<!--  />  -->
	<SolidColorBrush x:Key="Brush.Item.Disabled.Text"
			Color="Black"
			/>
	<SolidColorBrush x:Key="enabledBkg"
			Color="{Binding Color, Source={StaticResource Color.Blue.Light.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="enabledText"
			Color="Black"
			/>

	<!--  main header  -->
	<SolidColorBrush x:Key="mainHeaderTitleBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="pdHdrFillerBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Very.C}}"
			/>

	<!--  end header  -->
	<SolidColorBrush x:Key="endPointHdrBkg"
			Color="{Binding Color, Source={StaticResource Brush.Border.Std.C}}"
			/>
	<SolidColorBrush x:Key="endPointDtlBkg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Med.Light.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="epHeaderTitleBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Very.C}}"
			/>
	<SolidColorBrush x:Key="endPointBdr"
			Color="{Binding Color, Source={StaticResource Brush.Border.Std.C}}"
			/>

	<!--  detail  -->
	<SolidColorBrush x:Key="pdRowBdrBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Med.C}}"
			/>

	<LinearGradientBrush x:Key="Brush.Bdr.Shadow" StartPoint="0.5,0" EndPoint="0.5,1">
		<GradientStop Offset="0.89" Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Xtra.Very.C}}" />
		<GradientStop Offset=".990" Color="{Binding Color, Source={StaticResource Color.Gray.Light.Very.C}}" />
	</LinearGradientBrush>

	<!--  edit panel  -->
	<SolidColorBrush x:Key="editPanelBtnBkg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Med.C}}"
			/>
	<!--#endregion-->

	<!--  Styles  -->

	<!--#region > Styles: General-->

	<Style x:Key="Header.Base.Bdr"
			TargetType="Border"
			>
		<Setter Property="BorderBrush" Value="{StaticResource Brush.Border.Light.C}" />
		<Setter Property="BorderThickness" Value="0,0,1,1" />
		<Setter Property="Margin" Value="0" />
	</Style>

	<!--#endregion-->

	<!--#region > Styles: Header: Column Labels: Point Value-->

	<Style x:Key="Header.ColLables.Value.Lbl"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="White" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="Padding" Value="8,0,0,2" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>

	<Style x:Key="Header.ColLables.Value.Bdr"
			BasedOn="{StaticResource Header.Base.Bdr}"
			TargetType="Border"
			>
		<Setter Property="Grid.Row" Value="{StaticResource pointRow}" />
	</Style>


	<!--#endregion-->

	<!--#region > Styles: Header: Column Labels: Point Data-->

	<Style x:Key="pdBdrBase"
			TargetType="Border"
			>
		<Setter Property="BorderBrush" Value="{StaticResource Brush.Border.Light.C}" />
		<Setter Property="BorderThickness" Value="0,0,1,0" />
		<Setter Property="Margin" Value="0" />
	</Style>

	<Style x:Key="Header.ColLables.Data.Lbl"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="White" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="Padding" Value="8,0,0,2" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>

	<Style x:Key="Header.ColLables.Data.Bdr"
			BasedOn="{StaticResource pdBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Grid.Row" Value="{StaticResource Row.General.Value}" />
	</Style>

	<!--#endregion-->

	<!--#region > Styles: Header: End point-->

	<!--  must be before  -->
	<Style x:Key="Header.EndPoint.Data.Filler.Lbl"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="White" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="Padding" Value="8,0,0,2" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>

	<Style x:Key="Header.EndPoint.Data.Filler.Bdr"
			BasedOn="{StaticResource Header.Base.Bdr}"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource Header.EndPoint.Filler.Background.C}" />
		<Setter Property="BorderBrush" Value="{StaticResource endPointBdr}" />
	</Style>

	<Style x:Key="Header.EndPoint.Data.Lbl"
			BasedOn="{StaticResource Header.EndPoint.Data.Filler.Lbl}"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="Black" />
	</Style>

	<Style x:Key="Header.EndPoint.Data.Bdr"
			BasedOn="{StaticResource pdBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource endPointDtlBkg}" />
		<Setter Property="BorderBrush" Value="{StaticResource endPointBdr}" />
	</Style>



	<Style x:Key="Header.EndPoint.Value.Bdr"
			BasedOn="{StaticResource Header.Base.Bdr}"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource endPointDtlBkg}" />
		<Setter Property="BorderBrush" Value="{StaticResource endPointBdr}" />
	</Style>

	<Style x:Key="Header.EndPoint.Value.Lbl"
			BasedOn="{StaticResource Header.EndPoint.Data.Filler.Lbl}"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="Black" />
	</Style>


	<!--#endregion-->

	<!--#region > Styles: Field: Field.Pt.Value.Tbx-->

	<Style x:Key="Field.Pt.Value.Tbx"
			TargetType="{x:Type TextBox}"
			>
		<Setter Property="Background" Value="{StaticResource Normal.Disabled.Background}" />
		<Setter Property="BorderBrush" Value="{StaticResource Normal.Disabled.Background}" />
		<Setter Property="BorderThickness" Value="0" />
		<Setter Property="Foreground" Value="{StaticResource Normal.Inactive.Foreground}" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="IsEnabled" Value="False" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Style.Triggers>
			<DataTrigger Binding="{Binding IsSelected, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ListBoxItem}, AncestorLevel=1}}"
					Value="True"
					>
				<Setter Property="Background" Value="{StaticResource enabledBkg}" />
				<Setter Property="BorderBrush" Value="{StaticResource enabledBkg}" />
				<Setter Property="Foreground" Value="{StaticResource enabledText}" />
				<Setter Property="IsEnabled" Value="True" />
			</DataTrigger>
		</Style.Triggers>
	</Style>
	<!--#endregion-->

	<!--#region >> Styles: Field: Field.Pt.Data-->

	<Style x:Key="Field.Pt.Data.Tbx"
			TargetType="{x:Type TextBlock}"
			>
		<Setter Property="Background" Value="{DynamicResource Normal.Inactive.Background}" />
		<Setter Property="Foreground" Value="{DynamicResource Normal.Inactive.Foreground}" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="Padding" Value="4,2,2,5" />
		<Setter Property="TextAlignment" Value="Center" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>

	<Style x:Key="Field.Pt.Data.Filler"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource Point.Filler.Background.C}" />
		<Setter Property="BorderBrush" Value="{StaticResource Brush.Border.Std}" />
		<Setter Property="BorderThickness" Value="0,0,1,0" />
		<Setter Property="Grid.Row" Value="{StaticResource Row.General.Value}" />
		<Setter Property="Margin" Value="0,0,0,0.25" />
	</Style>

	<!--#endregion-->

	<!--#region >> Styles: Field: Editing Row-->

	<Style x:Key="Field.Ed.Grid"
			TargetType="UniformGrid"
			>
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="Visibility" Value="Visible" />

		<Style.Triggers>
			<DataTrigger Binding="{Binding IsBeingEdited, Mode=OneWay}"
					Value="true"
					>
				<Setter Property="Visibility" Value="Visible" />
			</DataTrigger>
		</Style.Triggers>
	</Style>

	<Style x:Key="Field.Ed.Button.Normal"
			BasedOn="{StaticResource Button.Normal.Style}"
			TargetType="Button"
			>
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Center" />
		<Setter Property="Margin" Value="-1,6,6,6" />
		<Setter Property="Width" Value="{StaticResource Field.Ed.Button.Width}" />

	</Style>


	<!--#endregion-->

	<!--#region >> Styles: Footer: Start point-->

	<Style x:Key="Ftr.StartPoint.Bdr"
			BasedOn="{StaticResource Header.EndPoint.Data.Bdr}"
			TargetType="Border"
			/>

	<Style x:Key="Ftr.StartPoint.Lbl"
			BasedOn="{StaticResource Header.EndPoint.Data.Lbl}"
			TargetType="{x:Type Label}"
			/>


	<Style x:Key="Ftr.StartPoint.Filler.Bdr"
			BasedOn="{StaticResource Header.EndPoint.Data.Filler.Bdr}"
			TargetType="Border"
			/>

	<Style x:Key="Ftr.StartPoint.Filler.Lbl"
			BasedOn="{StaticResource Header.EndPoint.Data.Filler.Lbl}"
			TargetType="{x:Type Label}"
			/>

	<!--#endregion-->

	<!--  Group Boxes  -->

	<!--#region >> GroupBox: Field.Pt.Value.Gbx-->
	<Style x:Key="Field.Pt.Value.Gbx"
			TargetType="{x:Type GroupBox}"
			>
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type GroupBox}">
					<Grid SnapsToDevicePixels="True">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="0" />
							<ColumnDefinition Width="*" />
							<ColumnDefinition Width="1" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="0" />
							<RowDefinition Height="*" />
							<RowDefinition Height="1" />

						</Grid.RowDefinitions>
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="0"
								BorderThickness="0,0,0,0"
								BorderBrush="{StaticResource Brush.Background.Std}"
								/>
						<Border Grid.Row="0"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,0"
								BorderBrush="{StaticResource Brush.Background.Std}"
								/>
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="2"
								BorderThickness="0,0,1,0"
								BorderBrush="{StaticResource Brush.Border.Std}"
								/>
						<Border Grid.Row="2"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,1"
								BorderBrush="{StaticResource Brush.Border.Std}"
								/>
						<ContentPresenter Grid.Row="1"
								Grid.Column="1"
								/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<!--#endregion-->

	<!--#region >> GroupBox: Field.Pt.Data.Gbx-->
	<Style x:Key="Field.Pt.Data.Gbx"
			TargetType="{x:Type GroupBox}"
			>
		
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type GroupBox}">
					<Grid SnapsToDevicePixels="True">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="0" />
							<ColumnDefinition Width="*" />
							<ColumnDefinition Width="1" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="0" />
							<RowDefinition Height="*" />
							<RowDefinition Height="0" />
						</Grid.RowDefinitions>

						<!--  left  -->
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="0"
								BorderThickness="1,0,0,0"
								BorderBrush="Transparent"
								/>
						<!--  top  -->
						<Border Grid.Row="0"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,1,0,0"
								BorderBrush="Transparent"
								/>
						<!--  right  -->
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="2"
								BorderThickness="0,0,1,0"
								BorderBrush="{StaticResource Brush.Border.Std}"
								/>
						<!--  bottom  -->
						<Border Grid.Row="2"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,1"
								BorderBrush="Transparent"
								/>

						<ContentPresenter Grid.Row="1"
								Grid.Column="1"
								/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!--#endregion-->

	<!--  Control Template  -->

	<!--#region >> Control Template: Header.Col.Labels-->
	<ControlTemplate x:Key="Header.ColLabels">
		<Grid Name="GridColLabels"
				Margin="0,0,0,-1"
				DockPanel.Dock="Top"
				Background="{StaticResource Header.Background.C}"
				>
			<Grid.ColumnDefinitions>
				<!--  0  -->
				<ColumnDefinition Name="GridColLabels_Col01"
						Width="{StaticResource Col.00.Width}"
						/>
				<!--  1  -->
				<ColumnDefinition Width="{StaticResource Col.01.Width}" />
				<!--  2  -->
				<ColumnDefinition Width="{StaticResource Col.02.Width}" />
				<!--  3  -->
				<ColumnDefinition Width="{StaticResource Col.03.Width}" />
				<!--  4  -->
				<ColumnDefinition Width="{StaticResource Col.04.Width}" />
				<!--  5  -->
				<ColumnDefinition Width="{StaticResource Col.05.Width}" />
				<!--  6  -->
				<ColumnDefinition Width="{StaticResource Col.06.Width}" />
				<!--  7  -->
				<ColumnDefinition Width="{StaticResource Col.07.Width}" />

			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="{StaticResource Row.Header.EndPoint.Height}" />
				<RowDefinition Height="{StaticResource Row.Header.EndPoint.Height}" />

			</Grid.RowDefinitions>

			<!--  Header rows  -->

			<!--#region >> Header: label rows-->

			<!--#region >> Header: Point value-->

			<Border Grid.Column="0"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.ColLables.Value.Bdr}"
					>
				<Label Content="X"
						Style="{StaticResource Header.ColLables.Value.Lbl}"
						/>
			</Border>
			<Border Grid.Column="2"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.ColLables.Value.Bdr}"
					>
				<Label Content="Y"
						Style="{StaticResource Header.ColLables.Value.Lbl}"
						/>
			</Border>
			<Border Grid.Column="4"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.ColLables.Value.Bdr}"
					>
				<Label Content="Z"
						Style="{StaticResource Header.ColLables.Value.Lbl}"
						/>
			</Border>

			<Border Grid.Column="6"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.ColLables.Value.Bdr}"
					>
				<Label Content="Slope"
						Style="{StaticResource Header.ColLables.Value.Lbl}"
						/>
			</Border>

			<!--#endregion-->

			<!--#region >> Header: Point data-->

			<Border Grid.Column="0"
					Background="{StaticResource Header.Filler.Background.C}"
					Style="{StaticResource Header.ColLables.Data.Bdr}"
					/>

			<Border Grid.Column="1"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.ColLables.Data.Bdr}"
					>
				<Label Content="Delta XY"
						Style="{StaticResource Header.ColLables.Data.Lbl}"
						/>
			</Border>

			<Border Grid.Column="3"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.ColLables.Data.Bdr}"
					>
				<Label Content="Delta XYZ"
						Style="{StaticResource Header.ColLables.Data.Lbl}"
						/>
			</Border>

			<Border Grid.Column="5"
					Grid.ColumnSpan="2"
					BorderThickness="0"
					Style="{StaticResource Header.ColLables.Data.Bdr}"
					>
				<Label Content="Delta Z"
						Style="{StaticResource Header.ColLables.Data.Lbl}"
						/>
			</Border>

			<Border Grid.Column="7"
					Background="{StaticResource Header.Filler.Background.C}"
					BorderThickness="0,0,0,1"
					Style="{StaticResource Header.ColLables.Data.Bdr}"
					/>

			<!--#endregion-->

			<!--#endregion-->

		</Grid>

	</ControlTemplate>


	<!--#endregion-->

	<!--#region >> Control Template: Header.EndPoint-->

	<ControlTemplate x:Key="Header.EndPoint">
		<Grid>
			<Grid.ColumnDefinitions>
				<!--  0  -->
				<ColumnDefinition Width="{StaticResource Col.00.Width}" />
				<!--  1  -->
				<ColumnDefinition Width="{StaticResource Col.01.Width}" />
				<!--  2  -->
				<ColumnDefinition Width="{StaticResource Col.02.Width}" />
				<!--  3  -->
				<ColumnDefinition Width="{StaticResource Col.03.Width}" />
				<!--  4  -->
				<ColumnDefinition Width="{StaticResource Col.04.Width}" />
				<!--  5  -->
				<ColumnDefinition Width="{StaticResource Col.05.Width}" />
				<!--  6  -->
				<ColumnDefinition Width="{StaticResource Col.06.Width}" />
				<!--  7  -->
				<ColumnDefinition Width="{StaticResource Col.07.Width}" />

			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="{StaticResource Row.Header.EndPoint.Height}" />
				<RowDefinition Height="{StaticResource Row.Header.EndPoint.Height}" />
			</Grid.RowDefinitions>

			<!--  Header: end point value row  -->

			<!--#region >> Header: End point value row-->

			<Border Grid.Row="0"
					Grid.Column="0"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.EndPoint.Value.Bdr}"
					>
				<Label Content="{Binding EndX}"
						Style="{StaticResource Header.EndPoint.Value.Lbl}"
						/>
			</Border>

			<Border Grid.Row="0"
					Grid.Column="2"
					Grid.ColumnSpan="2"
					Style="{StaticResource Header.EndPoint.Value.Bdr}"
					>
				<Label Content="{Binding EndY}"
						Style="{StaticResource Header.EndPoint.Value.Lbl}"
