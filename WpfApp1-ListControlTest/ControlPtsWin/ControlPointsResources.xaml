	</GridLength>
	<!--#endregion-->

	<SolidColorBrush x:Key="Header.Background.C"
			Color="{StaticResource gray.250}"
			/>
	<SolidColorBrush x:Key="Header.Filler.Background.C"
			Color="{StaticResource gray.200}"
			/>
	<SolidColorBrush x:Key="Header.EndPoint.Filler.Background.C"
			Color="{StaticResource gray.400}"
			/>

	<SolidColorBrush x:Key="Point.Filler.Background.C"
			Color="{StaticResource gray.400}"
			/>

	<SolidColorBrush x:Key="Row.Edit.Background.C"
			Color="{StaticResource gray.850}"
			/>



	<SolidColorBrush x:Key="ListBox.Background.C"
			Color="{StaticResource red.050}"
			/>
	<SolidColorBrush x:Key="ListBox.Grid.Border.C"
			Color="{StaticResource gray.450}"
			/>

	<SolidColorBrush x:Key="Color.Gray.Light.Very.C"
			Color="{StaticResource gray.970}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Light.Xtra.C"
			Color="{StaticResource gray.900}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Med.Light.Xtra.C"
			Color="{StaticResource gray.730}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Med.C"
			Color="{StaticResource gray.700}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Xtra.C"
			Color="{StaticResource gray.300}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Very.C"
			Color="{StaticResource gray.220}"
			/>
	<SolidColorBrush x:Key="Color.Gray.Dark.Xtra.Very.C"
			Color="{StaticResource gray.150}"
			/>

	<SolidColorBrush x:Key="Color.Blue.Light.Xtra.C"
			Color="{StaticResource blue.900}"
			/>



	<!--  general  -->

	<SolidColorBrush x:Key="stdBorderBrush"
			Color="{Binding Color, Source={StaticResource ListBox.Grid.Border.C}}"
			/>
	<SolidColorBrush x:Key="controlBkg"
			Color="{Binding Color, Source={StaticResource Normal.Background.C}}"
			/>
	<SolidColorBrush x:Key="dockpnlBkg"
			Color="LightGray"
			/>
	<SolidColorBrush x:Key="mainHeaderBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Very.C}}"
			/>
	<SolidColorBrush x:Key="disabledBkg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Light.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="disabledText"
			Color="Black"
			/>
	<SolidColorBrush x:Key="enabledBkg"
			Color="{Binding Color, Source={StaticResource Color.Blue.Light.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="enabledText"
			Color="Black"
			/>

	<!--  main header  -->
	<SolidColorBrush x:Key="mainHeaderTitleBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="pdHdrFillerBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Very.C}}"
			/>

	<!--  end header  -->
	<SolidColorBrush x:Key="endPointHdrBkg"
			Color="{Binding Color, Source={StaticResource ListBox.Grid.Border.C}}"
			/>
	<SolidColorBrush x:Key="endPointDtlBkg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Med.Light.Xtra.C}}"
			/>
	<SolidColorBrush x:Key="epHeaderTitleBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Very.C}}"
			/>
	<SolidColorBrush x:Key="endPointBdr"
			Color="{Binding Color, Source={StaticResource ListBox.Grid.Border.C}}"
			/>

	<!--  detail  -->
	<SolidColorBrush x:Key="pdRowBdrBg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Med.C}}"
			/>

	<LinearGradientBrush x:Key="shadow" StartPoint="0.5,0" EndPoint="0.5,1">
		<GradientStop Offset="0.89" Color="{Binding Color, Source={StaticResource Color.Gray.Dark.Xtra.Very.C}}" />
		<GradientStop Offset=".990" Color="{Binding Color, Source={StaticResource Color.Gray.Light.Very.C}}" />
	</LinearGradientBrush>

	<!--  edit panel  -->
	<SolidColorBrush x:Key="editPanelBtnBkg"
			Color="{Binding Color, Source={StaticResource Color.Gray.Med.C}}"
			/>


	<!--#endregion-->

	<!--#region > Styles-->

	<!--#region > Style: Point Value Row-->

	<Style x:Key="pvBdrBase"
			TargetType="Border"
			>
		<Setter Property="BorderBrush" Value="{StaticResource stdBorderBrush}" />
		<Setter Property="BorderThickness" Value="0,0,1,1" />
		<Setter Property="Margin" Value="0" />
	</Style>

	<Style x:Key="HdrPvRowLblStyle"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="White" />
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="Padding" Value="8,0,0,2" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
	</Style>

	<Style x:Key="HdrPvRowStyle"
			BasedOn="{StaticResource pvBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Grid.Row" Value="{StaticResource pointRow}" />
	</Style>

	<Style x:Key="ptvRowTbxStyle"
			TargetType="{x:Type TextBox}"
			>
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="BorderBrush" Value="{StaticResource Normal.Disabled.Background}" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<!-- <EventSetter Event="GotFocus" Handler="tbxXYZ_OnGotFocus"/> -->
		<!-- <EventSetter Event="LostFocus" Handler="tbxXYZ_OnLostFocus"/> -->
		<Setter Property="IsEnabled" Value="False" />
		<Setter Property="Foreground" Value="{StaticResource disabledText}" />
		<Setter Property="Background" Value="{StaticResource Normal.Disabled.Background}" />
		<Style.Triggers>
			<DataTrigger Binding="{Binding IsSelected, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ListBoxItem}, AncestorLevel=1}}"
					Value="True"
					>
				<Setter Property="IsEnabled" Value="True" />
				<Setter Property="Foreground" Value="{StaticResource enabledText}" />
				<Setter Property="Background" Value="{StaticResource enabledBkg}" />
				<Setter Property="BorderBrush" Value="{StaticResource enabledBkg}" />
			</DataTrigger>
		</Style.Triggers>
	</Style>

	<!--#endregion-->

	<!--#region > Style: Point data-->

	<Style x:Key="pdBdrBase"
			TargetType="Border"
			>
		<Setter Property="BorderBrush" Value="{StaticResource stdBorderBrush}" />
		<Setter Property="BorderThickness" Value="0,0,1,0" />
		<Setter Property="Margin" Value="0" />
	</Style>

	<Style x:Key="pdHdrRowLblStyle"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="White" />
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="Padding" Value="8,0,0,2" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
	</Style>

	<Style x:Key="HdrPdRowStyle"
			BasedOn="{StaticResource pdBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Grid.Row" Value="{StaticResource valueRow}" />
	</Style>

	<Style x:Key="ptdRowTbxStyle"
			TargetType="{x:Type TextBlock}"
			>
		<Setter Property="Background" Value="{DynamicResource Normal.Inactive.Background}" />
		<Setter Property="Foreground" Value="{DynamicResource Normal.Inactive.Foreground}" />
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="Padding" Value="4,2,2,5" />
		<Setter Property="TextAlignment" Value="Center" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
	</Style>

	<Style x:Key="Field.Pt.Data.Filler.Style"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource Point.Filler.Background.C}" />
		<Setter Property="Grid.Row" Value="{StaticResource valueRow}" />
		<Setter Property="BorderBrush" Value="{StaticResource stdBorderBrush}" />
		<Setter Property="BorderThickness" Value="0,0,1,0" />
		<Setter Property="Margin" Value="0,0,0,0.25" />
	</Style>

	<!--#endregion-->

	<!--#region > Styles: End point-->

	<Style x:Key="HdrEpFillerLblStyle"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="White" />
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="Padding" Value="8,0,0,2" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
	</Style>

	<Style x:Key="HdrEpvRowLblStyle"
			BasedOn="{StaticResource HdrEpFillerLblStyle}"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="Black" />
	</Style>
	<Style x:Key="HdrEpdRowLblStyle"
			BasedOn="{StaticResource HdrEpFillerLblStyle}"
			TargetType="{x:Type Label}"
			>
		<Setter Property="Foreground" Value="Black" />
	</Style>

	<Style x:Key="Header.EndPoint.Filler.Style"
			BasedOn="{StaticResource pvBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource Header.EndPoint.Filler.Background.C}" />
		<Setter Property="BorderBrush" Value="{StaticResource endPointBdr}" />
	</Style>

	<Style x:Key="HdrEpvRowStyle"
			BasedOn="{StaticResource pvBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource endPointDtlBkg}" />
		<Setter Property="BorderBrush" Value="{StaticResource endPointBdr}" />
	</Style>

	<Style x:Key="HdrEpdRowStyle"
			BasedOn="{StaticResource pdBdrBase}"
			TargetType="Border"
			>
		<Setter Property="Background" Value="{StaticResource endPointDtlBkg}" />
		<Setter Property="BorderBrush" Value="{StaticResource endPointBdr}" />
	</Style>

	<!--#endregion-->

	<!--#region > Style: Editing Row-->

	<Style x:Key="edRowGridStyle"
			TargetType="Grid"
			>
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="Visibility" Value="Visible" />

		<Style.Triggers>
			<DataTrigger Binding="{Binding IsBeingEdited, Mode=OneWay}"
					Value="true"
					>
				<Setter Property="Visibility" Value="Visible" />
			</DataTrigger>
		</Style.Triggers>
	</Style>

	<!--#endregion-->

	<!--#region > Styles: Footer: Start point-->

	<Style x:Key="FtrSpdRowStyle"
			BasedOn="{StaticResource HdrEpdRowStyle}"
			TargetType="Border"
			/>

	<Style x:Key="FtrSpdRowLblStyle"
			BasedOn="{StaticResource HdrEpdRowLblStyle}"
			TargetType="{x:Type Label}"
			/>


	<Style x:Key="Footer.EndPoint.Filler.Background"
			BasedOn="{StaticResource Header.EndPoint.Filler.Style}"
			TargetType="Border"
			/>

	<Style x:Key="FtrEpFillerLblStyle"
			BasedOn="{StaticResource HdrEpFillerLblStyle}"
			TargetType="{x:Type Label}"
			/>

	<!--#endregion-->

	<!--#endregion-->

	<!--#region > Group Boxes-->

	<!--#region > GroupBox: ptvRowGbxStyle-->
	<Style x:Key="ptvRowGbxStyle"
			TargetType="{x:Type GroupBox}"
			>
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type GroupBox}">
					<Grid SnapsToDevicePixels="True">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="0" />
							<ColumnDefinition Width="*" />
							<ColumnDefinition Width="1" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="0" />
							<RowDefinition Height="*" />
							<RowDefinition Height="1" />

						</Grid.RowDefinitions>
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="0"
								BorderThickness="0,0,0,0"
								BorderBrush="{StaticResource controlBkg}"
								/>
						<Border Grid.Row="0"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,0"
								BorderBrush="{StaticResource controlBkg}"
								/>
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="2"
								BorderThickness="0,0,1,0"
								BorderBrush="{StaticResource stdBorderBrush}"
								/>
						<Border Grid.Row="2"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,1"
								BorderBrush="{StaticResource stdBorderBrush}"
								/>
						<ContentPresenter Grid.Row="1"
								Grid.Column="1"
								/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<!--#endregion-->

	<!--#region > GroupBox: ptdRowGbxStyle-->
	<Style x:Key="ptdRowGbxStyle"
			TargetType="{x:Type GroupBox}"
			>
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type GroupBox}">
					<Grid SnapsToDevicePixels="True">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="0" />
							<ColumnDefinition Width="*" />
							<ColumnDefinition Width="1" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="0" />
							<RowDefinition Height="*" />
							<RowDefinition Height="0" />
						</Grid.RowDefinitions>

						<!--  left  -->
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="0"
								BorderThickness="1,0,0,0"
								BorderBrush="Transparent"
								/>
						<!--  top  -->
						<Border Grid.Row="0"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,1,0,0"
								BorderBrush="Transparent"
								/>
						<!--  right  -->
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="2"
								BorderThickness="0,0,1,0"
								BorderBrush="{StaticResource stdBorderBrush}"
								/>
						<!--  bottom  -->
						<Border Grid.Row="2"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,1"
								BorderBrush="Transparent"
								/>

						<ContentPresenter Grid.Row="1"
								Grid.Column="1"
								/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!--#endregion-->

	<!--#region > GroupBox Sep Color bottom row edit-->

	<Style x:Key="grpBxColoredSidesBottomEditable"
			TargetType="{x:Type GroupBox}"
			>
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type GroupBox}">
					<Grid SnapsToDevicePixels="True">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="0" />
							<ColumnDefinition Width="*" />
							<ColumnDefinition Width="1" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="0" />
							<RowDefinition Height="*" />
							<RowDefinition Height="0" />
						</Grid.RowDefinitions>

						<!--  left  -->
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="0"
								BorderThickness="1,0,0,0"
								BorderBrush="{StaticResource controlBkg}"
								/>
						<!--  top  -->
						<Border Grid.Row="0"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,1,0,0"
								BorderBrush="{StaticResource controlBkg}"
								/>
						<!--  right  -->
						<Border Grid.Row="0"
								Grid.RowSpan="3"
								Grid.Column="2"
								BorderThickness="0,0,1,0"
								BorderBrush="{StaticResource stdBorderBrush}"
								/>
						<!--  bottom  -->
						<Border Grid.Row="2"
								Grid.Column="0"
								Grid.ColumnSpan="3"
								BorderThickness="0,0,0,0"
								BorderBrush="{StaticResource controlBkg}"
								/>

						<ContentPresenter Grid.Row="1"
								Grid.Column="1"
								/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!--#endregion-->



	<!--#endregion-->


	<!--  Control Template  -->

	<!--#region > Control Template: Hdr.Col.Labels-->
	<ControlTemplate x:Key="Hdr.Col.Labels">
		<Grid Margin="0,0,0,-1"
				DockPanel.Dock="Top"
				Background="{StaticResource Header.Background.C}"
				>
			<Grid.ColumnDefinitions>
				<!--  0  -->
				<ColumnDefinition Width="{StaticResource col00}" />
				<!--  1  -->
				<ColumnDefinition Width="{StaticResource col01}" />
				<!--  2  -->
				<ColumnDefinition Width="{StaticResource col02}" />
				<!--  3  -->
				<ColumnDefinition Width="{StaticResource col03}" />
				<!--  4  -->
				<ColumnDefinition Width="{StaticResource col04}" />
				<!--  5  -->
				<ColumnDefinition Width="{StaticResource col05}" />
				<!--  6  -->
				<ColumnDefinition Width="{StaticResource col06}" />
				<!--  7  -->
				<ColumnDefinition Width="{StaticResource col07}" />

			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="{StaticResource hdrRowHeight}" />
				<RowDefinition Height="{StaticResource hdrRowHeight}" />

			</Grid.RowDefinitions>

			<!--  Header rows  -->

			<!--#region + Header: label rows-->

			<!--#region + Header: Point value-->

			<Border Grid.Column="0"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrPvRowStyle}"
					>
				<Label Content="X"
						Style="{StaticResource HdrPvRowLblStyle}"
						/>
			</Border>
			<Border Grid.Column="2"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrPvRowStyle}"
					>
				<Label Content="Y"
						Style="{StaticResource HdrPvRowLblStyle}"
						/>
			</Border>
			<Border Grid.Column="4"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrPvRowStyle}"
					>
				<Label Content="Z"
						Style="{StaticResource HdrPvRowLblStyle}"
						/>
			</Border>

			<Border Grid.Column="6"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrPvRowStyle}"
					>
				<Label Content="Slope"
						Style="{StaticResource HdrPvRowLblStyle}"
						/>
			</Border>

			<!--#endregion-->

			<!--#region + Header: Point data-->

			<Border Grid.Column="0"
					Background="{StaticResource Header.Filler.Background.C}"
					Style="{StaticResource HdrPdRowStyle}"
					/>

			<Border Grid.Column="1"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrPdRowStyle}"
					>
				<Label Content="Delta XY"
						Style="{StaticResource pdHdrRowLblStyle}"
						/>
			</Border>

			<Border Grid.Column="3"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrPdRowStyle}"
					>
				<Label Content="Delta XYZ"
						Style="{StaticResource pdHdrRowLblStyle}"
						/>
			</Border>

			<Border Grid.Column="5"
					Grid.ColumnSpan="2"
					BorderThickness="0"
					Style="{StaticResource HdrPdRowStyle}"
					>
				<Label Content="Delta Z"
						Style="{StaticResource pdHdrRowLblStyle}"
						/>
			</Border>

			<Border Grid.Column="7"
					Background="{StaticResource Header.Filler.Background.C}"
					BorderThickness="0,0,0,1"
					Style="{StaticResource HdrPdRowStyle}"
					/>

			<!--#endregion-->

			<!--#endregion-->

		</Grid>

	</ControlTemplate>


	<!--#endregion-->

	<!--#region > Control Template: Hdr.EndPoint.Value-->

	<ControlTemplate x:Key="Hdr.EndPoint.Value">
		<Grid>
			<Grid.ColumnDefinitions>
				<!--  0  -->
				<ColumnDefinition Width="{StaticResource col00}" />
				<!--  1  -->
				<ColumnDefinition Width="{StaticResource col01}" />
				<!--  2  -->
				<ColumnDefinition Width="{StaticResource col02}" />
				<!--  3  -->
				<ColumnDefinition Width="{StaticResource col03}" />
				<!--  4  -->
				<ColumnDefinition Width="{StaticResource col04}" />
				<!--  5  -->
				<ColumnDefinition Width="{StaticResource col05}" />
				<!--  6  -->
				<ColumnDefinition Width="{StaticResource col06}" />
				<!--  7  -->
				<ColumnDefinition Width="{StaticResource col07}" />

			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="{StaticResource hdrRowHeight}" />
				<RowDefinition Height="{StaticResource hdrRowHeight}" />
			</Grid.RowDefinitions>

			<!--  Header: end point pv row  -->

			<!--#region + Header: End point value row-->

			<Border Grid.Row="0"
					Grid.Column="0"
					Grid.ColumnSpan="2"
					Style="{StaticResource HdrEpvRowStyle}"
					>
				<Label Content="{Binding EndX}"
						Style="{StaticResource HdrEpvRowLblStyle}"
						/>
			</Border>

			<Border Grid.Row="0"
					Grid.Column="2"
					Grid.ColumnSpan="2"
