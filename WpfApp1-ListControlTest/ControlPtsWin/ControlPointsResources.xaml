	</sys:Int32>

	<sys:Double x:Key="Field.Ed.Button.Width">
		70
	</sys:Double>

	<GridLength x:Key="Row.Header.EndPoint.Height">
		18
	</GridLength>
	<GridLength x:Key="Field.Pt.Value.Row.Height">
		19
	</GridLength>
	<GridLength x:Key="Field.Pt.Data.Row.Height">
		18
	</GridLength>

	<GridLength x:Key="Row.Field.Ed.Height.Top">
