	<!--  Primary Template  -->

	<!--#region >> Primary Control Template: ListBox.Overall-->

	<ControlTemplate x:Key="ListBox.Overall">
		<!--  perimeter border  -->
		<Border Margin="0,0,0,0"
				BorderThickness="0"
				>

			<DockPanel Margin="0"
					LastChildFill="True"
					Background="{StaticResource Brush.DockPanel.Background}"
					>

				<!--#region > Main header w/ end point-->
				<Grid Margin="0,0,0,0"
						VerticalAlignment="Top"
						DockPanel.Dock="Top"
						>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="*" />
						<ColumnDefinition Width="17.5" />
					</Grid.ColumnDefinitions>
					<Grid.RowDefinitions>
						<RowDefinition Height="41" />
						<RowDefinition Height="40" />
					</Grid.RowDefinitions>

					<!--#region >> Header: Header.ColLabels-->
					<Border Grid.Row="0"
							Grid.Column="0"
							Margin="0,0,-1,-1.5"
							BorderThickness="0,0,0,8"
							BorderBrush="{StaticResource Brush.Bdr.Shadow}"
							>

						<ContentControl DockPanel.Dock="Top"
								Template="{StaticResource Header.ColLabels}"
								/>

					</Border>

					<!--#endregion-->

					<!--#region >> Header: Header.EndPoint-->
					<Border Grid.Row="1"
							Margin="0,0,-1,-2"
							VerticalAlignment="top"
							DockPanel.Dock="top"
							Background="{StaticResource Brush.Border.Std}"
							BorderThickness="0,0,0,6"
							BorderBrush="{StaticResource Brush.Bdr.Shadow}"
							>

						<ContentControl Template="{StaticResource Header.EndPoint}" />

					</Border>
					<!--  end end point header  -->
					<!--#endregion-->

					<!--#region >> Scroll bar column-->
					<Border Grid.Row="0"
							Grid.RowSpan="2"
							Grid.Column="1"
							Background="{StaticResource Header.Background.C}"
							BorderThickness="1,0,0,1"
							BorderBrush="{StaticResource Brush.Border.Std}"
							/>
					<!--#endregion-->

				</Grid>
				<!--  end main header  -->

				<!--#endregion-->

				<!--#region >> ScrollViewer w/ start point-->

				<!--  present the list box values  -->
				<ScrollViewer Name="ScrollViewer"
						Padding="0,0,0,0"
						HorizontalAlignment="Stretch"
						HorizontalContentAlignment="Stretch"
						VerticalScrollBarVisibility="Visible"
						
						
						>
					<Grid MaxWidth="{Binding ElementName=ScrollViewer, Path=ViewportWidth}"
							Margin="2,0,-3,0"
							
							>

						<Grid.RowDefinitions>
							<RowDefinition Height="Auto" />
							<RowDefinition Height="*" />
						</Grid.RowDefinitions>
						<ItemsPresenter Margin="-8,0,-5,0" 
								
								/>
						<ContentControl Grid.Row="1"
								Margin="0,2,0,0"
								VerticalAlignment="Top"
								
								Template="{StaticResource Ftr.StartPoint}"
								/>
					</Grid>

				</ScrollViewer>
				<!--  end  -->
				<!--#endregion-->

			</DockPanel>

		</Border>
		<!--  end perimeter border  -->

	</ControlTemplate>
	<!--#endregion-->

	<!--  Data Template  -->

	<!--#region >> Data Template: ListBox.Item.Selected.Normal-->

	<DataTemplate x:Key="ListBox.Item.Selected.Normal">
		<Border Margin="-1,0,-1,-4"
				BorderThickness="0,0,0,5"
				BorderBrush="{DynamicResource Brush.Bdr.Shadow}"
				
				>
			<Grid Margin="2,-1.5,-0.5,0"
					
					>
				<Grid.ColumnDefinitions>
					<!--  0  -->
					<ColumnDefinition Width="*" />
				</Grid.ColumnDefinitions>
				<Grid.RowDefinitions>
					<RowDefinition Height="{StaticResource Field.Pt.Value.Row.Height}" />
					<RowDefinition Height="{StaticResource Field.Pt.Data.Row.Height}" />
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>

				<!--#region + Point Value Row-->
				<ContentControl Grid.Row="{DynamicResource Field.Pt.Value.Row}"
						Grid.Column="0"
						
						Template="{DynamicResource Field.Pt.Value}"
						/>

				<!--#endregion-->

				<!--#region + Point Data row-->
				<ContentControl Grid.Row="{DynamicResource Field.Pt.Data.Row}"
						Grid.Column="0"
						Template="{DynamicResource Field.Pt.Data}"
						/>

				<!--#endregion-->

			</Grid>
		</Border>
	</DataTemplate>

	<!--#endregion-->

	<!--#region >> Data Template: ListBox.Item.Selected.Edit-->

	<DataTemplate x:Key="ListBox.Item.Selected.Edit">
		<Border Margin="0,0,-2,-6"
				BorderThickness="0,0,0,9"
				BorderBrush="{DynamicResource Brush.Bdr.Shadow}"
				>
			<Grid Margin="0,0,0,0">
				<Grid.ColumnDefinitions>
					<!--  0  -->
					<ColumnDefinition Width="*" />
				</Grid.ColumnDefinitions>
				<Grid.RowDefinitions>
					<RowDefinition Height="{StaticResource Field.Pt.Value.Row.Height}" />
					<RowDefinition Height="{StaticResource Field.Pt.Data.Row.Height}" />
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>

				<!--  Detail Value Row  -->
				<ContentControl Grid.Row="{DynamicResource Field.Pt.Value.Row}"
						Grid.Column="0"
						Template="{DynamicResource Field.Pt.Value}"
						/>

				<!--  Detail info row  -->
				<ContentControl Grid.Row="{DynamicResource Field.Pt.Data.Row}"
						Grid.Column="0"
						Template="{DynamicResource Field.Pt.Data}"
						/>

				<!--  EditPanel  -->
				<ContentControl Grid.Row="{DynamicResource Field.Ed.Row}"
						Grid.Column="0"
						Template="{DynamicResource Field.Ed}"
						/>

			</Grid>
		</Border>
	</DataTemplate>

	<!--#endregion-->

	<!--#region >> Data Template: Selector: ListBox.Item.Selector-->

	<DataTemplate x:Key="ListBox.Item.Selector">
		<ContentPresenter x:Name="contentPresenter" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Content}"
				/>
		<DataTemplate.Triggers>
			<DataTrigger Binding="{Binding IsBeingEdited, UpdateSourceTrigger=PropertyChanged}"
					Value="true"
					>
				<Setter TargetName="contentPresenter" Property="ContentTemplate" Value="{DynamicResource ListBox.Item.Selected.Edit}" />
			</DataTrigger>
			<DataTrigger Binding="{Binding IsBeingEdited, UpdateSourceTrigger=PropertyChanged}"
					Value="false"
					>
				<Setter TargetName="contentPresenter" Property="ContentTemplate" Value="{DynamicResource ListBox.Item.Selected.Normal}" />
			</DataTrigger>
		</DataTemplate.Triggers>

	</DataTemplate>

	<!--#endregion-->


</ResourceDictionary>


