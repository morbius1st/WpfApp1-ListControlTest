						<Grid.ColumnDefinitions>
							<!--  0  -->
							<ColumnDefinition Width="100" />
							<!--  1  -->
							<ColumnDefinition Width="100" />
							<!--  2  -->
							<ColumnDefinition Width="100" />
							<!--  3  -->
							<ColumnDefinition Width="100" />
							<!--  4  -->
							<ColumnDefinition Width="100" />
							<!--  5  -->
							<ColumnDefinition Width="100" />
							<!--  6  -->
							<ColumnDefinition Width="100" />
							<!--  7  -->
							<ColumnDefinition Width="100" />
							<!--  8  -->
							<ColumnDefinition Width="1*" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="25" />
							<RowDefinition Height="24" />
						</Grid.RowDefinitions>

						<Border Grid.Row="0"
								Grid.Column="0"
								Grid.ColumnSpan="2"
								Style="{StaticResource lblHeaderBdrTop}"
								>
							<TextBox Grid.Column="0"
									Text="{Binding SheetData}"
