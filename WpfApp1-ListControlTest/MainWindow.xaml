								>
							<TextBox Grid.Column="0"
									Text="{Binding X}"
									Style="{DynamicResource CellTxBox}"
									/>
						</Border>
						<Border Grid.Row="0"
								Grid.Column="2"
								Grid.ColumnSpan="1"
								Style="{StaticResource lblHeaderBdrTop}"
								>
							<TextBox Grid.Column="1"
									Text="{Binding Y}"
									Style="{DynamicResource CellTxBox}"
									/>
						</Border>
						<Border Grid.Row="0"
								Grid.Column="3"
								Grid.ColumnSpan="1"
								Style="{StaticResource lblHeaderBdrTop}"
								>
							<TextBox Text="{Binding Z}"
									Style="{DynamicResource CellTxBox}"
									/>
						</Border>

						<Border Grid.Row="0"
								Grid.Column="4"
								Grid.ColumnSpan="1"
								Style="{StaticResource lblHeaderBdrTop}"
								>
							<TextBlock Text="{Binding XΔ}"
